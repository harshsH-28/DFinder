<%- include("header.ejs"); %>



<section id="testimonials">


  <div id="testimonial-carousel" class="carousel slide" data-ride="carousel data-interva="1000">
  <div class="carousel-inner">
    <div class="carousel-item">
      <div class="testimonials-division">
      <h2 class="testimonial-text"></h2>
      <img src="<%= img %>" alt="">
      </div>
    </div>
    <div class="carousel-item active">
        <div class="testimonials-division">
        <h2>Covid</h2>
        <p>You Have Covid</p>
        </div>
    </div>
    <div class="carousel-item">
      <div class="testimonials-division">
      <h2>Severe</h2>
      <p>You Have a covid condition with severity of 6 out of 10</p>
    </div>
    </div>
  </div>
  <a class="carousel-control-prev" href="#testimonial-carousel" role="button" data-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="carousel-control-next" href="#testimonial-carousel" role="button" data-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>
</section>


<%- include("footer.ejs"); %>
